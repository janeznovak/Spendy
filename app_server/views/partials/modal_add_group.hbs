<div
            class="modal fade"
            id="dodajSkupino"
            tabindex="-1"
            role="dialog"
            aria-labelledby="exampleModalLabelSkupina"
            aria-hidden="true"
    >
    <form method="post" id="formaModala">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabelSkupina">
                        Dodajte skupino
                    </h5>
                    <button
                            type="button"
                            class="close"
                            data-dismiss="modal"
                            aria-label="Close"
                    >
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form
                            action="#"
                            class="form"
                            id="registrationForm"
                            method="post"
                    >
                        <div class="form-group">
                            <div class="col-xs-6">
                                <label for="first_name"><h4>Ime skupine</h4></label>
                                <input
                                        class="form-control"
                                        id="imeSKupine"
                                        type="text"
                                        placeholder="Neko ime"
                                        name="imeZaSkupino"
                                        pattern="^[a-zA-Z\s]*$"
                                />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-6">
                                <label for="last_name"><h4>Limit</h4></label>
                                <input
                                        class="form-control"
                                        id="limit"
                                        name="last_name"
                                        placeholder="xyz €"
                                        type="text"
                                />
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-xs-6">
                                <label for="phone"
                                ><h4>Uporabniško ime drugega čana</h4></label
                                >
                                <input
                                        class="form-control"
                                        id="drugiClan"
                                        type="text"
                                        placeholder="janko7"
                                        name="imeUporabnika"
                                        pattern="^[a-zA-Z\s]*$"
                                />
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button
                            type="button"
                            class="btn btn-secondary"
                            data-dismiss="modal"
                    >
                        Preklic
                    </button>
                    <button type="submit" class="btn btn-primary" onclick="balidate()">
                        Dodajte skupino
                    </button>
                </div>
            </div>
        </div>
    </form>
    </div>